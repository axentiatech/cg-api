{
  "name": "cg-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "dev:wrangler": "wrangler dev src/index.ts",
    "dev:esbuild": "esbuild --bundle src/server.ts --format=esm --watch --outfile=dist/_worker.js",
    "build:wrangler": "esbuild --bundle src/server.ts --format=esm --outfile=dist/_worker.js",
    "build": "tsc && tsc-alias",
    "deploy": "wrangler deploy --minify"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "@hono/node-server": "^1.13.2",
    "@hono/zod-openapi": "^0.16.4",
    "@upstash/vector": "^1.1.7",
    "ai": "^3.4.9",
    "dotenv": "^16.4.5",
    "hono": "^4.6.3",
    "hono-pino": "^0.3.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "stoker": "^1.0.9",
    "tsc-alias": "^1.8.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "wrangler": "^3.91.0"
  }
}
